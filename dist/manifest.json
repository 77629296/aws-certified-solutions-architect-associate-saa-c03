{
  "name": "Chrome Extension React Starter",
  "description": "Chrome Extension React Starter",
  "version": "1.0.0",
  "manifest_version": 3,
  "homepage_url": "https://github.com/77629296/online-video-helper",
  "background": {
    "service_worker": "scripts/background/main.js"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["scripts/content/main.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "popup"
  },
  "icons": {
    "16": "images/icon-16x16.png",
    "48": "images/icon-48x48.png",
    "128": "images/icon-128x128.png"
  },
  "options_page": "apps/options/index.html",
  "permissions": [ "storage", "tabs", "contextMenus", "unlimitedStorage" ],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [ {
    "matches": [ "*://*/*" ],
    "resources": [ "*" ]
 } ]
}
